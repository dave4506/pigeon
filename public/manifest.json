{
  "manifest_version": 2,
  "name": "Pigeon",
  "description": "Browse the web without browsing.",
  "version": "1.0",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
